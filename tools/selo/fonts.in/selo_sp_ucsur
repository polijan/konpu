#include "../../../src/glyph_constants/glyph_tokipona.h"

#ifndef   SITELENPONA_SIZE
#  define SITELENPONA_SIZE 6
#endif

#if SITELENPONA_SIZE <= 8
#   define SITELENPONA(x)  UTIL_CAT(UTIL_CAT(UTIL_CAT(GLYPH64_TOKIPONA, SITELENPONA_SIZE), _), x)
#else
#   error "Wrong SITELENPONA_SIZE"
#endif


