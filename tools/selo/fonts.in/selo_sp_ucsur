//------------------------------------------------------------------------------
// Punctuation and format controls
//------------------------------------------------------------------------------
F1990   SITELENPONA(CARTOUCHE1_)      // START OF CARTOUCHE
F1991   SITELENPONA(_CARTOUCHE1)      // END OF CARTOUCHE
F1992   SITELENPONA(_CARTOUCHE1_)     // COMBINING CARTOUCHE EXTENSION
F1993   SITELENPONA(PI_)              // START OF LONG PI
F1994   SITELENPONA(UNDERLINE)        // COMBINING LONG PI EXTENSION
F1995   SITELENPONA(JOINER_STACKING)  // STACKING JOINER
F1996   SITELENPONA(JOINER_SCALING)   // SCALING JOINER
F1997   GLYPH64(F808000000000000)     // START OF LONG GLYPH
F1998   GLYPH64(1F10000000000000)     // END OF LONG GLYPH
F1999   SITELENPONA(UNDERLINE)        // COMBINING LONG GLYPH EXTENSION
F199A   GLYPH64(F808000000000000)     // START OF REVERSE LONG GLYPH
F199B   GLYPH64(1F10000000000000)     // END OF REVERSE LONG GLYPH
F199C   SITELENPONA(DOT)              // MIDDLE DOT
F199D   SITELENPONA(COLON)            // COLON
// F199E ... F199F  (This position shall not be used)

