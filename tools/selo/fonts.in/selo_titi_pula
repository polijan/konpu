// Titi Tula: Tuki Tiki's Logograms

#include "../../../src/glyph_constants/glyph_tukitiki.h"

#ifndef    TITIPULA_SIZE
#   define TITIPULA_SIZE 7
#endif

#if TITIPULA_SIZE <= 8
#   define TITIPULA(x)  UTIL_CAT(UTIL_CAT(UTIL_CAT(GLYPH64_TUKITIKI, TITIPULA_SIZE), _), x)
#else
#   error "Wrong TITIPULA_SIZE"
#endif

//------------------------------------------------------------------------------
// UCSUR Titi Pula (as of 2024-04-11)
// https://www.kreativekorp.com/ucsur/charts/titi-pula.html
// PUA-A Codepoints U+F1C40...U+F1C17
//------------------------------------------------------------------------------
F1C40  TITIPULA(A)
F1C41  TITIPULA(ALA)
F1C42  TITIPULA(I)
F1C43  TITIPULA(IKU)
F1C44  TITIPULA(ILU)
F1C45  TITIPULA(KA)
F1C46  TITIPULA(KATI)
F1C47  TITIPULA(KI)
F1C48  TITIPULA(KIKU)
F1C49  TITIPULA(KU)
F1C4A  TITIPULA(LA)
F1C4B  TITIPULA(LAPI)
F1C4C  TITIPULA(LI)
F1C4D  TITIPULA(LIKA)
F1C4E  TITIPULA(LILI)
F1C4F  TITIPULA(LU)
F1C50  TITIPULA(LUPA)
F1C51  TITIPULA(MI)
F1C52  TITIPULA(MUKU)
F1C53  TITIPULA(MUTI)
F1C54  TITIPULA(PAKA)
F1C55  TITIPULA(PALI)
F1C56  TITIPULA(PUKA)
F1C57  TITIPULA(PULA)
F1C58  TITIPULA(TAKA)
F1C59  TITIPULA(TAMA)
F1C5A  TITIPULA(TIKI)
F1C5B  TITIPULA(TIKU)
F1C5C  TITIPULA(TILA)
F1C5D  TITIPULA(TILU)
F1C5E  TITIPULA(TIMI)
F1C5F  TITIPULA(TIPI)
F1C60  TITIPULA(TITI)
F1C61  TITIPULA(TU)
F1C62  TITIPULA(TUKI)
F1C63  TITIPULA(TULA)
F1C64  TITIPULA(TULU)
F1C65  TITIPULA(ULI)
F1C66  TITIPULA(UPI)
//F1C67...F1C7F  (This position shall not be used)

